  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddRepairItem</servlet-name>
    <url-pattern>/AddRepairItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DisposeRepair</servlet-name>
    <url-pattern>/DisposeRepair</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DoneRepair</servlet-name>
    <url-pattern>/DoneRepair</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
