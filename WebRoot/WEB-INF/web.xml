    <servlet-class>cn.repair.servlet.repairlog.QueryRepairLog</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddRepairLog</servlet-name>
    <servlet-class>cn.repair.servlet.repairlog.AddRepairLog</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddRepairItem</servlet-name>
    <servlet-class>cn.repair.servlet.repairitem.AddRepairItem</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DisposeRepair</servlet-name>
    <servlet-class>cn.repair.servlet.repairitem.DisposeRepair</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DoneRepair</servlet-name>
    <servlet-class>cn.repair.servlet.repairitem.DoneRepair</servlet-class>
  </servlet>




	<filter>
		<filter-name>loginFilter</filter-name>
		<filter-class>cn.repair.core.LoginFilter</filter-class>
	</filter>
	<!--映射过滤器 
	<filter-mapping>
		<filter-name>loginFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>-->








  <servlet-mapping>
    <servlet-name>UserLogin</servlet-name>
    <url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryUsers</servlet-name>
    <url-pattern>/QueryUsers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SetManage</servlet-name>
    <url-pattern>/SetManage</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateUser</servlet-name>
    <url-pattern>/UpdateUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EditUserPassword</servlet-name>
    <url-pattern>/EditUserPassword</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegUser</servlet-name>
    <url-pattern>/RegUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryRepairItem</servlet-name>
    <url-pattern>/QueryRepairItem</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
